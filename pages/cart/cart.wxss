.out-side-cart {
  background-color: #f4f4f4;
  min-height: 100vh;
}

.out-side-cart .header-address {
  width: 280rpx;
  height: 50rpx;
  line-height: 50rpx;
  padding-left: 35rpx;
  font-size: 28rpx;
  color: #fff;
  position: absolute;
  left: 20rpx;
  top: 19rpx;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -344rpx / 70rpx 1000rpx;
}

.out-side-cart .header-cart-btn {
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #fff;
  position: absolute;
  right: 20rpx;
  top: 19rpx;
}

.out-side-cart .cart-main {
  background-color: #fff;
  overflow: hidden;
  padding: 88rpx 0 100rpx;
}

.out-side-cart .cart-main .cart-number {
  height: 66rpx;
  line-height: 66rpx;
  font-size: 28rpx;
  color: #1a1a1a;
  padding-left: 20rpx;
}

.out-side-cart .cart-list {
  width: 710rpx;
  height: 250rpx;
  box-shadow: 0 0 5rpx 5rpx rgba(0, 0, 0, .03);
  margin: 0 auto 20rpx;
}

.out-side-cart .cart-list .check-single {
  width: 42rpx;
  height: 32rpx;
  float: left;
  margin: 110rpx 0 0 12rpx;
  overflow: hidden;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -64rpx / 45rpx 643rpx;
}

.out-side-cart .cart-list .check-single.this-over {
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -30rpx / 45rpx 643rpx;
}

.out-side-cart .cart-list .cart-img {
  width: 200rpx;
  height: 190rpx;
  overflow: hidden;
  border-radius: 10rpx;
  float: left;
  margin: 30rpx 15rpx 0 0;
}

.out-side-cart .cart-list .cart-img image {
  width: 200rpx;
  height: 200rpx;
}

.out-side-cart .cart-list .cart-mess-bar {
  width: 430rpx;
  height: 210rpx;
  float: left;
  margin-top: 26rpx;
  position: relative;
}

.out-side-cart .cart-list .cart-mess-bar .name {
  font-size: 28rpx;
  color: #262626;
}

.out-side-cart .cart-list .cart-mess-bar .spec {
  display: inline-block;
  font-size: 22rpx;
  color: #b2b2b2;
  position: absolute;
  top: 80rpx;
}

.out-side-cart .cart-list .cart-mess-bar .return {
  display: inline-block;
  font-size: 40rpx;
  color: #fe7600;
  position: absolute;
  left: -45rpx;
  top: 110rpx;
  padding: 2rpx 6rpx;
  border: 1px solid #fe7600;
  border-radius: 2px;
  transform: scale(0.5);
}

.out-side-cart .cart-list .cart-mess-bar .price {
  display: inline-block;
  font-size: 38rpx;
  color: #e62343;
  position: absolute;
  top: 158rpx;
  line-height: 38rpx;
}

.out-side-cart .cart-list .cart-mess-bar .cart-btn-box {
  display: inline-block;
  position: absolute;
  right: 0;
  bottom: 16rpx;
}

.out-side-cart .cart-list .cart-mess-bar .cart-btn-box text {
  width: 50rpx;
  display: inline-block;
  padding: 8rpx 0;
  font-size: 22rpx;
  color: #262626;
  float: right;
  border-top: 1px solid #dcdcdc;
  border-left: 1px solid #dcdcdc;
  border-bottom: 1px solid #dcdcdc;
  text-align: center;
  overflow: hidden;
}

.out-side-cart .cart-list .cart-mess-bar .cart-btn-box .jia {
  border-right: 1px solid #dcdcdc;
  border-bottom-right-radius: 5rpx;
  border-top-right-radius: 5rpx;
  color: #a0a0a0;
}

.out-side-cart .cart-list .cart-mess-bar .cart-btn-box .jian {
  border-bottom-left-radius: 5rpx;
  border-top-left-radius: 5rpx;
  color: #a0a0a0;
}

.out-side-cart .cart-list .cart-mess-bar .cart-btn-box .jian:active,
.out-side-cart .cart-list .cart-mess-bar .cart-btn-box .jia:active {
  background-color: #ececec;
}

.out-side-cart .cart-footer {
  position: fixed;
  width: 100vw;
  height: 105rpx;
  border-top: 1px solid #eee;
  left: 0;
  bottom: 0;
  overflow: hidden;
  background-color: #fff;
}

.out-side-cart .to-pay {
  width: 212rpx;
  height: 105rpx;
  line-height: 105rpx;
  text-align: center;
  position: absolute;
  right: 0;
  top: 0;
  background-color: #e62343;
  font-size: 28rpx;
  color: #fff;
}

.out-side-cart .to-pay.gray {
  background-color: #ccc;
}

.out-side-cart .cart-footer-mess {
  padding-right: 230rpx;
}

.out-side-cart .cart-footer-mess .t {
  padding-top: 10rpx;
  text-align: right;
}

.out-side-cart .cart-footer-mess.single .t {
  padding-top: 0;
  line-height: 105rpx;
}

.out-side-cart .cart-footer-mess .t .f-type1 {
  font-size: 26rpx;
  color: #1a1a1a;
}

.out-side-cart .cart-footer-mess .t .f-type2 {
  font-size: 34rpx;
  color: #e62343;
}

.out-side-cart .cart-footer-mess .t .f-type3 {
  font-size: 22rpx;
  color: #595959;
}

.out-side-cart .cart-footer-mess .b {
  padding-top: 5rpx;
  text-align: right;
  font-size: 22rpx;
  color: #595959;
}

.out-side-cart .btn-select-all {
  display: inline-block;
  position: absolute;
  padding-left: 40rpx;
  font-size: 28rpx;
  height: 35rpx;
  overflow: hidden;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -63rpx / 45rpx 643rpx;
  left: 10rpx;
  top: 38rpx;
}

.out-side-cart .btn-select-all.this-over {
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -29rpx / 45rpx 643rpx;
}

.out-side-cart .btn-cart-del,
.out-side-cart .btn-cart-move {
  display: inline-block;
  font-size: 26rpx;
  color: #999;
  border: 1px solid #999;
  padding: 10rpx 30rpx;
  float: right;
  margin: 22rpx 20rpx 0 0;
  border-radius: 60rpx;
}

.out-side-cart .btn-cart-del {
  color: #e62343;
  border: 1px solid #e62343;
}

.cart-null {
  text-align: center;
}

.cart-null .cart-null-icon {
  padding: 280rpx 0 50rpx;
}

.cart-null .cart-null-icon image {
  width: 308rpx;
  height: 224rpx;
}

.cart-null .note1 {
  font-size: 36rpx;
  color: #404040;
}

.cart-null .note2 {
  font-size: 28rpx;
  color: #666;
  padding: 20rpx 0 50rpx;
}

.cart-null .cart-null-btn text {
  display: inline-block;
  font-size: 36rpx;
  color: #e62343;
  border: 1px solid #e62343;
  border-radius: 80rpx;
  padding: 20rpx 180rpx;
  margin: 0 auto;
}
.base-box {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 101
}
.base-box .gray-back {
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: .7;
  position: absolute;
  left: 0;
  top: 0
}
.base-box .box-confirm {
  width: 600rpx;
  overflow: hidden;
  border-radius: 20rpx;
  position: absolute;
  left: 75rpx;
  top: calc(50vh - 200rpx);
}
.base-box .box-confirm .txt {
  padding: 60rpx 20rpx;
  font-size: 32rpx;
  line-height: 44rpx;
  text-align: center;
  background-color: #fff;
}
.base-box .box-confirm .btn {
  height: 130rpx;
  border-top: 1px solid #eee;
}
.base-box .box-confirm .btn text {
  display: inline-block;
  width: 300rpx;
  height: 130rpx;
  line-height: 130rpx;
  text-align: center;
  font-size: 34rpx;
  float: left;
  background-color: #fff;
}
.base-box .box-confirm .btn text.l:active {
  background-color: #f5f5f5;
}
.base-box .box-confirm .btn text.r {
  background-color: #e62343;
  color: #fff;
}